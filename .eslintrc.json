{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "amd": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-redux/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "react-redux"],
  "rules": {
    "no-console": 1,
    "no-inline-comments": 1,
    "block-scoped-var": 1,
    "consistent-return": 1,
    "curly": 1,
    "default-case": 1,
    "dot-notation": 1,
    "eqeqeq": 1,
    "no-alert": 1,
    "no-caller": 1,
    "no-constructor-return": 1,
    "no-else-return": 1,
    "no-empty-function": 1,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-extra-label": 1,
    "no-floating-decimal": 1,
    "no-implicit-coercion": 1,
    "no-implicit-globals": 1,
    "no-implied-eval": 1,
    "no-iterator": 1,
    "no-labels": 1,
    "no-magic-numbers": [1, { "ignore": [0, 1] }],
    "no-multi-str": 1,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-param-reassign": 1,
    "no-return-assign": 1,
    "no-return-await": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 1,
    "no-unmodified-loop-condition": 1,
    "no-unused-expressions": 1,
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-useless-return": 1,
    "no-void": 1,
    "prefer-promise-reject-errors": 1,
    "prefer-regex-literals": 1,
    "radix": 1,
    "require-await": 1,
    "vars-on-top": 1,
    "wrap-iife": 1,
    "no-use-before-define": 1,
    "callback-return": 1,
    "global-require": 1,
    "no-buffer-constructor": 1,
    "no-mixed-requires": 1,
    "no-new-require": 1,
    "no-path-concat": 1,
    "no-var": 1,
    "arrow-parens": [1, "as-needed"],
    "no-duplicate-imports": 1,
    "no-useless-computed-key": 1,
    "no-useless-constructor": 1,
    "no-useless-rename": 1,
    "prefer-const": 1,
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "prefer-template": 1,
    "symbol-description": 1,
    "no-dupe-else-if": 1,
    "no-import-assign": 1,
    "no-setter-return": 1,
    "no-template-curly-in-string": 1,
    "require-atomic-updates": 1,
    "react-redux/prefer-separate-component-file": 0,
    "react-redux/connect-prefer-named-arguments": 0
  }
}
